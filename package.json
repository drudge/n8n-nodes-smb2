{
  "name": "n8n-nodes-smb2",
  "version": "0.2.0",
  "description": "Transfer files via Samba (SMB2) with n8n",
  "keywords": [
    "n8n-community-node-package",
    "smb2",
    "smb",
    "samba",
    "binary",
    "file",
    "transfer",
    "node",
    "n8n"
  ],
  "license": "MIT",
  "homepage": "https://github.com/drudge/n8n-nodes-smb2",
  "author": {
    "name": "Nick Penree",
    "email": "nick@penree.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drudge/n8n-nodes-smb2.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Smb2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Smb2/Smb2.node.js",
      "dist/nodes/Smb2Trigger/Smb2Trigger.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3",
    "@types/node": "^18.10.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "node-smb2": "^1.3.1",
    "tmp-promise": "^3.0.3"
  }
}
